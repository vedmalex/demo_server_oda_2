{
  "name": "api-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "watch": "tsc --build tsconfig.json --watch",
    "postinstall_": "npm run compile",
    "generate": "node dist/generate.js",
    "validate": "node dist/validate.js",
    "genUml": "node dist/genUml.js",
    "debug": "node --inspect-brk dist/app.js",
    "updateSchema": "node updateSchema.js",
    "compile": "tsc --build tsconfig.json",
    "clean": "git clean -dfqX -- ./node_modules **/{dist,node_modules}/ ./packages/*/tsconfig*tsbuildinfo",
    "collector_old": "cd src/graphql-gen/system/dataQueries; graphql-document-collector '**/*.graphql' > ../../../../data/seed-queries.json; cd ../../../.. ; node dist/seeds/validate.js",
    "validate_schema": "node dist/seeds/validate.js",
    "collector": "cd src/graphql-gen/system/dataQueries; graphql-document-collector '**/*.graphql' > ../../../../data/seed-queries.json; cd ../../../.. ",
    "initData": "node dist/seeds/restoreDirect.js data/dump.json",
    "restoreData": "node dist/seeds/restoreDirect.js data/dump_1.json",
    "dumpData": "node dist/seeds/dumpDirect.js data/dump_1.json",
    "db": "run-rs --keep"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link-batch-http": "^1.2.12",
    "apollo-server": "^2.8.1",
    "dataloader": "^1.4.0",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.5",
    "mongoose": "^5.6.8",
    "oda-api-common": "^1.0.0-17",
    "oda-api-graphql": "^1.0.0-17",
    "oda-api-graphql-mongoose": "^1.0.0-17",
    "oda-gen-common": "^1.0.0-17",
    "oda-gen-graphql": "^1.0.0-17",
    "oda-lodash": "^1.0.0-17",
    "oda-logger": "1.0.1-alpha.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/graphql": "^14.2.3",
    "@types/inflected": "^1.1.29",
    "@types/mongoose": "^5.5.11",
    "@types/node": "^12.6.9",
    "@types/passport": "^1.0.0",
    "graphql-document-collector": "^0.2.2",
    "typescript": "^3.5.3",
    "run-rs": "0.5.2"
  }
}
